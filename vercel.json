{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Frontend/dist",
        "installCommand": "cd Frontend && npm install --unsafe-perm && npm install -g vite",
        "buildCommand": "cd Frontend && npx --yes --package=vite@latest vite build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Frontend/dist/$1"
    }
  ]
}

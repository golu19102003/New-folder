{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Frontend/dist",
        "installCommand": "chmod +x scripts/vercel-build.sh && ./scripts/vercel-build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Frontend/dist/$1"
    }
  ]
}

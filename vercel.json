{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "installCommand": "cd Frontend && npm install --unsafe-perm && npx tsc --version",
        "buildCommand": "cd Frontend && npx tsc && vite build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Frontend/dist/$1"
    },
    {
      "src": "/Frontend/(.*)",
      "dest": "/Frontend/$1"
    }
  ]
}
